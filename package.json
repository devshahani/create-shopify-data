{
  "name": "create-shopify-data",
  "description": "Generate data for your Shopify store",
  "version": "0.1.1",
  "author": "dev shahani @devshahani",
  "bin": {
    "create-shopify-data": "./bin/run"
  },
  "bugs": "https://github.com/devshahani/create-shopify-data/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.18.0",
    "configstore": "^4.0.0",
    "faker": "^4.1.0",
    "keytar": "^4.3.0",
    "listr": "^0.14.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "shopify-api-node": "^2.15.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/configstore": "^2.1.1",
    "@types/faker": "^4.1.4",
    "@types/keytar": "^4.0.1",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.118",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "shopify-api-node-interface": "^2.0.0",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/devshahani/create-shopify-data",
  "keywords": [
    "developer tools",
    "shopify",
    "data generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "create-shopify-data",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "devshahani/create-shopify-data",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
